<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>
    <!--数据库类型-->
    <add key="DBType" value="mssql"/>
    <!--数据库自增-->
    <add key="AutoIncrement" value="false"/>
  </appSettings>
  <connectionStrings>
    <!--数据库连接-->
    <add name="DefaultConnection" connectionString="Data Source=sqlite.db;Version=3;"/>
    <add name="DBHelper" connectionString="Data Source=localhost\SQLEXPRESS;Initial Catalog=DBHelper;Persist Security Info=True;User ID=sa;Password=123456" providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>
